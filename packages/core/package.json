{
  "name": "gm-route-replay-core",
  "version": "1.0.0",
  "description": "Core playback logic for gm-route-replay",
  "keywords": [
    "google-maps",
    "gps",
    "route",
    "replay",
    "playback",
    "animation",
    "webgl",
    "visualization"
  ],
  "author": "skyt-a",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyt-a/gm-route-replay.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/skyt-a/gm-route-replay/issues"
  },
  "homepage": "https://skyt-a.github.io/gm-route-replay/ja/",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
