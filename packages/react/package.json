    "google-maps",
    "route",
    "replay",
    "react",
    "visualization"
  ],
  "author": "skyt-a",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyt-a/gm-route-replay.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/skyt-a/gm-route-replay/issues"
  },
