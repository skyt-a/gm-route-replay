{
  "name": "@gm/route-replay-react",
  "version": "0.1.0",
  "private": true,
  "description": "React hook and components for gm-route-replay",
  "keywords": [
    "google-maps",
    "route",
    "replay",
    "react",
    "hook"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
